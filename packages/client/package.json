{
    "name": "scrpr-client",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "deploy": "serverless deploy -v",
        "deploy:ui": "yarn build && deploy-aws-s3-cloudfront --bucket scrpr-client-bucket --source=./build --non-interactive",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "undeploy": "./scripts/undeploy.sh && serverless remove"
    },
    "dependencies": {
        "amazon-cognito-identity-js": "^3.0.14",
        "classnames": "^2.2.6",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.1.0",
        "scrpr-api": "1.0.0",
        "styled-components": "^4.3.2"
    },
    "devDependencies": {
        "@storybook/react": "^5.1.9",
        "@types/classnames": "^2.2.9",
        "@types/jest": "^24.0.15",
        "@types/node": "^12.6.8",
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.4",
        "@types/react-router-dom": "^4.3.4",
        "@types/storybook__react": "^4.0.2",
        "@types/styled-components": "4.1.8",
        "awesome-typescript-loader": "^5.2.1",
        "deploy-aws-s3-cloudfront": "^1.0.0",
        "http-proxy-middleware": "^0.19.1",
        "node-sass": "^4.12.0",
        "react-docgen-typescript-loader": "^3.1.0",
        "serverless": "^1.52.2",
        "serverless-stack-output": "^0.2.3"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
